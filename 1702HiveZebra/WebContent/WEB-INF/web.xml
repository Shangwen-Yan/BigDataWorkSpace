<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>APP01Servlet</servlet-name>
    <servlet-class>cn.zebra.web.APP01Servlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>APP02Servlet</servlet-name>
    <servlet-class>cn.zebra.web.APP02Servlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ZookeeperState</servlet-name>
    <servlet-class>cn.zebra.web.ZookeeperState</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>APP01Servlet</servlet-name>
    <url-pattern>/servlet/APP01Servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>APP02Servlet</servlet-name>
    <url-pattern>/servlet/APP02Servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ZookeeperState</servlet-name>
    <url-pattern>/servlet/ZookeeperState</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
